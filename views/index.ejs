    <div class="my-2" style="text-align: center; color: #d3e0ea">&#9670&ensp;&ensp;&#9670&ensp;&ensp;&#9670</div>

    <%- include('./partials/footer') %>

    <div id="login-box-holder" class="m-0 px-4 myModalHolder d-none">

      <div id="login-box" class="mx-auto p-4 myModal">
        <div class="m-0 p-0 d-none loader">
          <img src="images/loading.svg" height="60px" width="60px" alt="Loading">
        </div>
        <i class="fas fa-times mb-3 ml-auto d-block Close" title="Close" data-parent="login-box"></i>
        <div class="row my-0 mx-auto" style="justify-content: center;">
          <div class="p-0 col-11">
            <span class="d-block mt-4 mb-1 ml-1 placeholder"
          >ph-No<i class="fas fa-phone-alt ml-1"></i
        ></span>
        <input class="mx-0 px-3 py-2 d-block col-12" type="text" name="phNo" form="login-form" required minlength="10" maxlength="10"/>
          </div>

          <div class="p-0 col-11">
            <span class="d-block mt-3 mb-1 ml-1 placeholder"
          >Password<i class="fas fa-lock ml-1"></i
        ></span>
        <input
        id="login-form-password"
          class="mx-0 px-3 py-2 d-block col-12"
          type="password"
          name="password" form="login-form" required
        />
        <i class="fas fa-eye password-toggle" data-toggle="#login-form-password"></i>
          </div>
        </div>
        
        
        <button class="mt-4 mb-1 py-2 px-3 enter-buttons" form="login-form">LOGIN</button>
      </div>
    </div>

    <div id="signup-box-holder" class="m-0 px-4 myModalHolder d-none">
      <div id="signup-box" class="mx-auto px-4 pt-4 pb-3 myModal">
        <div class="m-0 p-0 d-none loader">
          <img src="images/loading.svg" height="60px" width="60px" alt="Loading">
        </div>
        <i class="fas fa-long-arrow-alt-left fa-lg mb-3 d-none back-arrow" title="Go Back"></i>
        <i class="fas fa-times mb-3 d-block Close" title="Close" data-parent="signup-box"></i>

        <div id="signup1" class="row mb-0 mt-3 mx-auto signup-child-box signup-active" style="justify-content: center;">
        

          <div class="p-0 col-8" style="display: inline-block">
            <span class="d-block mt-0 mb-1 ml-1 placeholder"
            >Your Name<i class="fas fa-signature fa-lg ml-1"></i
            ></span>
            <input class="mx-0 px-3 py-2 d-block col-12" type="text" name="name" form="signup1-form" required/>
          </div>
        
          <div class="p-0 ml-3 col-3 col-sm-2" style="display: inline-block">
            <span class="d-block mt-0 mb-1 ml-1 placeholder"
            >Age<i class="fas fa-sort-numeric-up-alt ml-1"></i
            ></span>
            <input class="mx-0 px-3 py-2 d-block col-12" type="text" name="age" form="signup1-form" required maxlength="3" />
          </div>

          <div class="p-0 col-11 col-sm-10">
            <span class="d-block mt-3 mb-1 ml-2 placeholder"
            >ph-No<i class="fas fa-phone-alt ml-1"></i
            ></span>
            <input class="mx-0 px-3 py-2 d-block col-12" type="text" name="phNo" form="signup1-form" required minlength="10" maxlength="10" />
          </div>

          <div class="p-0 col-11 col-sm-10">
            <span class="d-block mt-3 mb-1 ml-1 placeholder"
              >Password<i class="fas fa-lock ml-1"></i
            ></span>
            <input
            id="signup-form-password"
              class="mx-0 px-3 py-2 d-block col-12"
              type="password"
              name="password" form="signup1-form" required
            />
            <i class="fas fa-eye password-toggle" data-toggle="#signup-form-password"></i>
          </div>

          <div class="p-0 col-11 col-sm-10">
            <span class="d-block mt-3 mb-1 ml-1 placeholder"
              >Confirm Password<i class="fas fa-lock ml-1"></i
            ></span>
            <input
            id="signup-form-confirmPassword"
              class="mx-0 px-3 py-2 d-block col-12"
              type="password"
              name="confirmPassword" form="signup1-form" required
            />
            <i class="fas fa-eye password-toggle" data-toggle="#signup-form-confirmPassword"></i>
          </div>

          <button class="mt-4 mb-1 py-2 px-3 enter-buttons signup-next" form="signup1-form">Next</button>
        </div>
        
        <div id="signup2" class="row mb-0 mt-3 mx-auto signup-child-box slide-right" style="justify-content: center;">
          <div class="p-0 col-11">
            <span class="d-block mt-0 mb-2 ml-1 placeholder"
            >Sex<i class="fas fa-venus-mars fa-lg ml-1"></i
            ></span>
            <div class="p-0 col-12" style="clear: both">
              <input class="myRadio" id="male" type="radio" name="sex" value="Male" form="signup2-form" required/>
              <label for="male">Male</label>
              <input class="ml-2 ml-sm-3 myRadio" id="female" type="radio" name="sex" value="Female" form="signup2-form" required/>
              <label for="female">Female</label>
              <input class="ml-2 ml-sm-3 myRadio" id="other" type="radio" name="sex" value="Other" form="signup2-form" required/>
              <label for="other">Other</label>
            </div>
            
          </div>

          <div class="p-0 col-11" style="display: inline-block">
            <span class="d-block mt-2 mb-1 ml-1 placeholder"
            >Address<i class="fas fa-map-pin fa-lg ml-1"></i
            ></span>
            <textarea class="mx-0 px-3 py-2 d-block col-12" name="address" rows="3" form="signup2-form" required></textarea>
          </div>

          <div class="p-0 col-11">
            <span class="d-block mt-2 mb-2 ml-1 placeholder"
            >Marital status<i class="fas fa-ring fa-lg ml-1"></i
            ></span>
            <div class="p-0 col-12" style="clear: both;">
              <input class="myRadio" id="married" type="radio" name="maritalStatus" value="Married" form="signup2-form" required/>
              <label class="col-4 px-0" for="married">Married</label>
              <input class="myRadio" id="unmarried" type="radio" name="maritalStatus" value="Unmarried" form="signup2-form" required/>
              <label class="col-4 px-0" for="unmarried">Unmarried</label>
            </div>
            <div class="p-0 col-12" style="clear: both">
              <input class="myRadio" id="divorced" type="radio" name="maritalStatus" value="Divorced" form="signup2-form" required/>
              <label class="col-4 px-0" for="divorced">Divorced</label>
              <input class="myRadio" id="widowed" type="radio" name="maritalStatus" value="Widowed" form="signup2-form" required/>
              <label class="col-4 px-0" for="widowed">Widowed</label>
            </div>
            
          </div>

          <div class="p-0 col-11">
            <span class="d-block mt-2 mb-2 ml-1 placeholder"
            >Religion<i class="fas fa-praying-hands fa-lg ml-1"></i
            ></span>
            <div class="p-0 col-12" style="clear: both">
              <input class="myRadio" id="hindu" type="radio" name="religion" value="Hindu" form="signup2-form" required/>
              <label for="hindu">Hindu</label>
              <input class="ml-2 ml-sm-3 myRadio" id="christian" type="radio" name="religion" value="Christian" form="signup2-form" required/>
              <label for="christian">Christian</label><br>
              <input class="ml-2 ml-sm-3 myRadio" id="muslim" type="radio" name="religion" value="Muslim" form="signup2-form" required/>
              <label for="muslim">Muslim</label>
            </div>
            
          </div>
          <button class="mt-2 mb-0 py-2 px-3 enter-buttons signup-next" form="signup2-form" required>Next</button>
        </div>
        
        <div id="signup3" class="row mb-0 mt-3 mx-auto signup-child-box slide-right" style="justify-content: center;">

          <div class="p-0 col-11">
            <span class="d-block mt-0 mb-2 ml-1 placeholder"
            >Educational Qualification<i class="fas fa-graduation-cap fa-lg ml-1"></i
            ></span>
            <select class="form-select mx-0 px-3 py-2 d-block col-12" name="educationalQualification" aria-label="Default select example" form="signup3-form" required>
              <option value="">Select your highest qualification</option>
              <% for(let i = 0; i < educationList.length; ++i){ %>
                <option value=<%= educationList[i]._id %>><%= educationList[i].education %></option>
              <% } %>
            </select>
          </div>

          <div class="p-0 col-11">
            <span class="d-block mt-2 mb-2 ml-1 placeholder"
            >Occupation<i class="fas fa-briefcase fa-lg ml-1"></i
            ></span>
            <div class="p-0 col-12" style="clear: both; text-align: left; white-space: nowrap;">
              <% for(let i = 0; i < occupationList.length; ++i){ %>
                <input class="ml-2 ml-sm-3 myRadio" id=<%= occupationList[i]._id %> type="radio" name="occupation" value=<%= occupationList[i]._id %> form="signup3-form" required/>
                <label for=<%= occupationList[i]._id %>><%= occupationList[i].occupation %></label><br>
              <% } %>
            </div>
          </div>

          <div class="p-0 col-11 col-sm-10">
            <span class="d-block mt-2 mb-2 ml-1 placeholder"
            >Total monthly family income<i class="fas fa-rupee-sign ml-1"></i
            ></span>
            <input class="mx-0 px-3 py-2 d-block col-6" type="text" name="monthlyIncome" form="signup3-form" required />
          </div>

          <button class="mt-3 mb-2 py-2 px-3 enter-buttons signup-next" form="signup3-form">Sign-Up</button>

        </div>
     
      </div>
    </div>
    <form action="/login" id="login-form" method="post"></form>
    <form onsubmit="return false" action="/" id="signup1-form" method="post"></form>
    <form onsubmit="return false" action="/" id="signup2-form" method="post"></form>
    <form onsubmit="return false" action="/" id="signup3-form" method="post"></form>
    <form action="/signup" id="signup-form" method="post"></form>

    <script
      src="https://code.jquery.com/jquery-3.6.0.min.js"
      integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
      crossorigin="anonymous"
    ></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script src="js/control.js"></script>
  </body>
</html>


